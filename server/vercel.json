{
    "version": 2,
    "builds": [{"src": "./index.js", "use": "@vercel/node"}],
    "routes": [
        {
            "src": "./(.*)", 
            "dest": "./public/index.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "./index.js"
          },
        {
            "src": "/api/history",
            "dest": "./index.js"
          }

    ]
}